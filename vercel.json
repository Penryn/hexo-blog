{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "headers": [
    {
      "source": "/((?!.*\\.js$)(?!.*\\.css$)(?!.*\\.png$)(?!.*\\.jpg$)(?!.*\\.jpeg$)(?!.*\\.webp$)(?!.*\\.avif$)(?!.*\\.svg$)(?!.*\\.ico$)(?!.*\\.woff$)(?!.*\\.woff2$)(?!.*\\.ttf$)(?!.*\\.otf$)(?!.*\\.eot$)(?!.*\\.map$)(?!.*\\.xml$)(?!.*\\.txt$).*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, s-maxage=3600, stale-while-revalidate=86400"
        }
      ]
    },
    {
      "source": "/(sitemap.xml|robots.txt|rss2.xml|local-search.xml|xml/local-search.xml)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, s-maxage=3600, stale-while-revalidate=86400"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|png|jpg|jpeg|webp|avif|svg|ico|woff|woff2|ttf|otf|eot|map)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
