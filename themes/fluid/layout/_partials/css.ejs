<link rel="preload" as="style" href="<%= url_join(theme.static_prefix.bootstrap, 'css/bootstrap.min.css') %>" onload="this.onload=null;this.rel='stylesheet'">
<noscript><%- css_ex(theme.static_prefix.bootstrap, 'css/bootstrap.min.css') %></noscript>

<% var css_snippets = deduplicate(page.css_snippets) %>
<% for (var idx = 0; idx < css_snippets.length; idx++) { %>
  <%- css_snippets[idx] %>
<% } %>
<% page.css_snippets = [] %>

<!-- 主题依赖的图标库，不要自行修改 -->
<!-- Do not modify the link that theme dependent icons -->
<link rel="preload" as="style" href="<%= url_for('/css/iconfont-local-1749284.css') %>" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="<%= url_for('/css/iconfont-local-1749284.css') %>"></noscript>

<link rel="preload" as="style" href="<%= url_for(theme.static_prefix.iconfont || theme.iconfont) %>" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="<%= url_for(theme.static_prefix.iconfont || theme.iconfont) %>"></noscript>

<link rel="preload" as="style" href="<%= url_join(theme.static_prefix.internal_css, 'main.css') %>" onload="this.onload=null;this.rel='stylesheet'">
<noscript><%- css_ex(theme.static_prefix.internal_css, 'main.css') %></noscript>

<% if (theme.code.highlight.enable) { %>
  <link id="highlight-css" rel="preload" as="style" href="<%= url_join(theme.static_prefix.internal_css, 'highlight.css') %>" onload="this.onload=null;this.rel='stylesheet'">
  <noscript><%- css_ex(theme.static_prefix.internal_css, 'highlight.css', 'id="highlight-css"') %></noscript>
  <% if (theme.dark_mode.enable) { %>
    <link id="highlight-css-dark" rel="preload" as="style" href="<%= url_join(theme.static_prefix.internal_css, 'highlight-dark.css') %>" onload="this.onload=null;this.rel='stylesheet'">
    <noscript><%- css_ex(theme.static_prefix.internal_css, 'highlight-dark.css', 'id="highlight-css-dark"') %></noscript>
  <% } %>
<% } %>

<% if (theme.custom_css) { %>
  <% var customCSS = Array.isArray(theme.custom_css) ? theme.custom_css : [theme.custom_css] %>
  <% for (var cssPath of customCSS) { %>
    <link rel="preload" as="style" href="<%= url_for(cssPath) %>" onload="this.onload=null;this.rel='stylesheet'">
    <noscript><link rel="stylesheet" href="<%= url_for(cssPath) %>"></noscript>
  <% } %>
<% } %>

<% if (page.layout === 'links') { %>
  <%- css_ex(theme.static_prefix.internal_css, 'link.css') %>
<% } %>
